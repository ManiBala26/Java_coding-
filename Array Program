	public static void main(String[] args) {
		int count=0;
		int a[] = {1,2,3,4,5};
		System.out.println("Odd number:");
		for (int i = 0; i < a.length; i++) {
	if (a[i]%2!=0) {
		System.out.println(a[i]);
		
	}
		}
	System.out.println("even NUmber:");
	for (int j = 0; j < a.length; j++) {
		
	 if(a[j]%2==0) {
		System.out.println(a[j]);
	}
					
					
				
		}	
				
				
			}
			
			
			
			
			
			
			
			
			
		
		
		
		
		
